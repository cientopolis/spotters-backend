<p id="notice"><%= notice %></p>

<h1>Message Votes</h1>

<table>
  <thead>
    <tr>
      <th>Positive</th>
      <th>User</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @message_votes.each do |message_vote| %>
      <tr>
        <td><%= message_vote.positive %></td>
        <td><%= message_vote.user %></td>
        <td><%= message_vote.message %></td>
        <td><%= link_to 'Show', message_vote %></td>
        <td><%= link_to 'Edit', edit_message_vote_path(message_vote) %></td>
        <td><%= link_to 'Destroy', message_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message Vote', new_message_vote_path %>
